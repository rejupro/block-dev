{
  "apiVersion": 2,
  "name": "blockdev/firstblock",
  "version": "0.1.0",
  "title": "First Block",
  "category": "common",
  "icon": "smiley",
  "description": "Example block written with ESNext standard and JSX support build step required.",
  "supports": {
    "html": false,
    "anchor": true,
    "align": true
  },
  "attributes": {
    "text": {
      "type": "string",
      "default": "Hello World"
    },
    "tags": {
      "type": "string",
      "default": "h2"
    },
    "textcolor": {
      "type": "string",
      "default": "#000000"
    },
    "backgroundcolor": {
      "type": "string",
      "default": "#ffffff"
    },
    "divPaddings": {
      "type": "object",
      "default": {
        "top": "10px",
        "bottom": "10px",
        "left": "10px",
        "right": "10px"
      }
    }
  },
  "textdomain": "boilerplate",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}